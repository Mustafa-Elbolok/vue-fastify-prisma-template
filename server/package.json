{
  "name": "server",
  "version": "0.0.1",
  "description": "simple server for creating/updating/deleting categories and apis using express mysql and prisma",
  "main": "index.js",
  "scripts": {
    "build": "tsc",
    "dev": "tsx watch src/index.ts",
    "start": "node ./dist/index.js",
    "db:format": "npx prisma format --schema=./src/prisma/schema.prisma",
    "db:seed": "node --loader ts-node/esm ./src/prisma/seed.ts",
    "db:migrate": "npx prisma migrate dev --schema=./src/prisma/schema.prisma --name=migration",
    "db:studio": "npx prisma studio --schema=./src/prisma/schema.prisma",
    "test": "jest",
    "test:watch": "ts-jest --watch",
    "test:e2e": "ts-jest --config ./jest-e2e.json"
  },
  "type": "module",
  "author": "Mustafa Elbolok (callmekmo1@gmail.com)",
  "license": "ISC",
  "dependencies": {
    "@fastify/cookie": "^9.3.1",
    "@fastify/cors": "^9.0.1",
    "@fastify/formbody": "^7.4.0",
    "@fastify/helmet": "^11.1.1",
    "@fastify/jwt": "^8.0.0",
    "@fastify/middie": "^8.3.0",
    "@fastify/multipart": "^8.2.0",
    "@fastify/static": "^7.0.1",
    "@fastify/type-provider-json-schema-to-ts": "^3.0.0",
    "@fastify/type-provider-typebox": "^4.0.0",
    "@prisma/client": "^5.11.0",
    "@sinclair/typebox": "^0.32.19",
    "bcryptjs": "^2.4.3",
    "fastify": "^4.26.2",
    "fastify-file-upload": "^4.0.0",
    "fluent-json-schema": "^4.2.1",
    "pino": "^8.19.0",
    "prisma": "^5.11.0"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.24.3",
    "@babel/preset-typescript": "^7.24.1",
    "@jest/globals": "^29.7.0",
    "@types/bcryptjs": "^2.4.6",
    "@types/cors": "^2.8.17",
    "@types/jest": "^29.5.12",
    "@types/jsonwebtoken": "^9.0.6",
    "@types/node": "^20.11.30",
    "@types/supertest": "^6.0.2",
    "dotenv": "^16.4.5",
    "jest": "^29.7.0",
    "prettier": "^3.2.5",
    "supertest": "^6.3.4",
    "ts-jest": "^29.1.2",
    "ts-node": "^10.9.2",
    "tsx": "^4.7.1",
    "typescript": "^5.4.3"
  }
}
